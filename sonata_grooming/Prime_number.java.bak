class Prime_number 
{
	public static void main(String[] args) 
	{
		System.out.println("To check Prime number or not");
		int a = 14;
		
		int flag  = 0;
		if (a==0 || a==1)
		{
			System.out.println("Number is not prime");
		}else
{
		for(int i = 2;i<=a/2;i++)
		{
			if(a%i==0)
			{
				System.out.println(a+" :Number is not prime ");
				flag = 1;
				break;
			}
		}
		if(flag ==0 )
		{
			System.out.println(a+" :Number is prime");
		}
	}

	}
}
