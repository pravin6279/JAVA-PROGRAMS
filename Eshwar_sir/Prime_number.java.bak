class Prime_number 
{
	public static void main(String[] args) 
	{
		int n = 19;
		int flag = 0;

		if (n == 0 && n == 1)
		{
			flag = 1;
		}
		else 
		{
			for (int i =2 ;i <= n/2;i++ )
			{
				if (i%n==0)
				{
					    flag = 1;
					    break;
				}
			}

		   
		}
			 if (flag == 0)
				{
					System.out.println("Number is prime number");
				}
				else
					{
			           System.out.println("Nunber not prime number");
					}

	}
}				
